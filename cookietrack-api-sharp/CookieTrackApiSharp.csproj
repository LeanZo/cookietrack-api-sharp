<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CookieTrackApiSharp</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>LeanZo</Authors>
    <Description>CookieTrack API wrapper for .NET</Description>
    <PackageProjectUrl>https://github.com/LeanZo/cookietrack-api-sharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/LeanZo/cookietrack-api-sharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>cookietrack, defi, crypto</PackageTags>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

		<ItemGroup>
				<None Include="LICENSE.txt" Pack="true" PackagePath="LICENSE.txt"/>
		</ItemGroup>

</Project>
